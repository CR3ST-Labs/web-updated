---
import HeaderLink from "./HeaderLink.astro";
import Button from "./atoms/Button.astro";
// import { SITE_TITLE } from "../consts";
import ArrowRightIcon from "./SVG/ArrowRightIcon.astro";
---

<header class="px-40 py-10">
  <nav class="flex justify-between items items-center">
    <div class="flex gap-10">
      <a href="/">
        <img class="hidden dark:block w-24" src="/images/branding/logo-light.svg" alt="" />
        <img class="dark:hidden w-24" src="/images/branding/logo.svg" alt="" />
      </a>
      <div class="flex items-center gap-8">
        <HeaderLink href="/">Home</HeaderLink>
        <HeaderLink href="/blog">Blog</HeaderLink>
        <HeaderLink href="/about">About</HeaderLink>
      </div>
    </div>

    <div>
      <Button>Get Started! <ArrowRightIcon class="ml-2" /> </Button>
    </div>
  </nav>
</header>
